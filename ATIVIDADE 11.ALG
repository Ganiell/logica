algoritmo "atividade 11"
var
n1, n2, n3, media  : real

inicio
       escreva("Escreva sua primeira nota")
       leia(n1)
       escreva("Escreva sua segunda nota")
       leia(n2)
       escreva("Escreva sua terceira nota")
       leia(n3)
       media <- ((n1*2+n2*3+n3*5)/10)
       escreva("Sua media é ", media)


fimalgoritmo