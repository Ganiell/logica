algoritmo "ler os numeros e somar"

var
    soma, conta, numero : inteiro
inicio
    conta <- 1
    soma <- 0

    enquanto (conta <= 5) faca
    escreva("Digite o ",conta,"° número: ")
    leia(numero)
    soma <- soma + numero
    conta <- conta + 1
    fimenquanto
    escreva("A soma dos números é",soma)
fimalgoritmo