algoritmo "muito importante"
var
   A, B, troca : inteiro

inicio
      escreva("Digite o valor de A: ")
      leia(A)
      escreva("Digite o valor de B: ")
      leia(B)
      troca <- A
      A <- B
      B <- troca
      escreval("O novo valor de A é",A)
      escreva("O novo valor de B é",B)
fimalgoritmo