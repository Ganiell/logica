algoritmo "atividade 7"

var

n1, n2 : inteiro


inicio
      escreva("Digite a temperatura do ambiente: ")
      leia(n1)
      n2 <- 25
      



      SE n1>n2 ENTAO
      escreva("Esta muito quente")
      SENAO
        SE n2>n1 ENTAO
      escreva("Esta agradavel")


      FIMSE
       FIMSE




fimalgoritmo